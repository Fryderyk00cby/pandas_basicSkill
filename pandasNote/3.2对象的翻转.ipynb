   ],
   "source": [
    "v= [[53,'男'],[64,'女'],[25,'女'],[44,'男'],[35,'女']]\n",
    "i = ['1号','2号','3号','4号','5号']\n",
    "c = ['年龄','性别']\n",
    "df = pd.DataFrame(v,index=i,columns=c)\n",
    "print(df)"
   ]
